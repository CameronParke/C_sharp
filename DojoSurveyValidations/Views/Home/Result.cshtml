@model User 
<div class="container">
        <h2><u>Here is @Model.name's Submitted Info</u></h2>
        <div class="form-group">
            <h3>Name: @Model.name</h3>
        </div>
        <div class="form-group">
            <h3>Dojo Location: @Model.location</h3>
        </div>
        <div class="form-group"> 
            <h3>Favorite Language: @Model.language</h3>
        </div>
        <div class="form-group">
            <h3>Comment: @Model.comment</h3>
            </div>
        <div class="form-group">
            <a href="http://127.0.0.1:5000/">
                <button value="Go Back" class="btn btn-primary">Go Back</button>
            </a>
        </div>
    </div>